<%= render 'projects/form', project: @project %>

<ul class="tabs" data-tabs id="example-tabs">
    <li class="tabs-title is-active">
        <a href="#panel1" aria-selected="true">Profile</a>
    </li>
    <li class="tabs-title">
        <a href="#panel2">Created Projects</a>
    </li>
    <li class="tabs-title">
        <a href="#panel3">Posted Logs</a>
    </li>
    <li class="tabs-title">
        <a href="#panel4">Comments</a>
    </li>
</ul>

<div class="tabs-content" data-tabs-content="example-tabs">
    <div class="tabs-panel is-active" id="panel1">
        <%= render 'info' %>
    </div>
    <div class="tabs-panel" id="panel2">
        <%= render 'projects/thumbs', projects: @projects %>
    </div>
    <div class="tabs-panel" id="panel3">
      <div class="medium-2">
        <%= render 'logs/list', logs: @logs %>
      </div>
    </div>
    <div class="tabs-panel" id="panel4">
    </div>
</div>
